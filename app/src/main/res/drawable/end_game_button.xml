<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/colorTransparent60" />
            <stroke android:width="2dp" android:color="@color/colorWhite" />
            <corners android:radius="0dp" />
            <padding android:bottom="2dp" android:left="5dp" android:right="5dp" android:top="2dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/colorTransparent60" />
            <stroke android:width="2dp" android:color="@color/colorNeutralStatistic" />
            <corners android:radius="0dp" />
            <padding android:bottom="2dp" android:left="5dp" android:right="5dp" android:top="2dp" />
        </shape>
    </item>
</selector>